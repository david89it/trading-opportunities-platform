{
  "name": "asymmetric-alpha-scanner",
  "version": "0.1.0",
  "description": "Asymmetric Alpha Scanner & Analytics Platform - A trading analytics tool for identifying asymmetric risk/reward opportunities",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.6",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{js,ts,tsx,json,md}\"",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build",
    "setup": "pnpm install && pnpm run build"
  },
  "devDependencies": {
    "@turbo/gen": "^1.10.16",
    "prettier": "^3.1.1",
    "turbo": "^1.10.16"
  },
  "keywords": [
    "trading",
    "analytics",
    "stocks",
    "risk-management",
    "asymmetric-opportunities",
    "scanner"
  ],
  "license": "MIT",
  "author": {
    "name": "Trading Analytics Team"
  }
}